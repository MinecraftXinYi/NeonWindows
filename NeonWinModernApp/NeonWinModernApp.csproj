<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard1.2</TargetFramework>
		<RootNamespace>NeonWindows</RootNamespace>
		<LangVersion>10.0</LangVersion>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<ProductVersion>0.4.7.0</ProductVersion>
		<FileVersion>$(ProductVersion)</FileVersion>
		<CheckNotRecommendedTargetFramework>false</CheckNotRecommendedTargetFramework>
		<NoWarn>$(NoWarn);NETSDK1215</NoWarn>
	</PropertyGroup>
	<ItemGroup>
	  <Folder Include="ABI\ApplicationModel\" />
	  <Folder Include="ABI\UI\Composition\" />
	  <Folder Include="ApplicationModel\" />
	  <Folder Include="UI\Immersive\" />
	</ItemGroup>

</Project>
